          BEGIN
               VC_DADOS := VC_DADOS + STGR_COMBINACOES_15.Cells[VI_CONTAR,VI_COMBINACOES1] + ' ';
          END;// FOR VI_CONTAR
          FOR VI_CONTAR := 1 TO 10 DO
          BEGIN
               VMS_NaoSorteados [VI_CONTAR] :=STGR_COMBINACOES_15.Cells[16+VI_CONTAR,VI_COMBINACOES1];
          END;// FOR VI_CONTAR
                  // **  Ordena os dados   **
                  FOR VI_CONTAR :=1 TO 25 DO
                  BEGIN
                      STRGR_TODOS_NUMEROS.Cells[VI_CONTAR,1] :='F';
                  END; // FOR VI_CONTAR
                  // ----> MONTADO E VERIFICANDO TODOS OS 23 DGITOS
                  FOR VI_CONTAR :=1 TO 10 DO
                  BEGIN
                      STRGR_TODOS_NUMEROS.Cells [ STRtoINT(TRIM(VMS_NaoSorteados[VI_CONTAR])) ,1]:='V';
                  END; // FOR VI_CONTAR1
                  VI_CNTCELULAS := 1;
