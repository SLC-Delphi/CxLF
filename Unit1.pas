     LA_LER_GRAVAR_PERCENTUAL_TEXTO1.Caption := '.';
     PGBR_LER_GRAVAR_PERCENTUAL.Position := 0;
     PA_LER_GRAVAR_PERCENTUAL.Left := 7750;
