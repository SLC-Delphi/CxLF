     VI_COMBINACOES1, VI_COMBINACOES2, VI_CONTAR, VI_CNTCELULAS : integer;
     VC_DADOS, VC_DADOS_NS, VC_QUERY : STRING;
     VMS_NaoSorteados: ARRAY [1..10] OF STRING;
BEGIN
     IF NOT(SELF.IBTransaction1.Active) THEN SELF.IBTransaction1.StartTransaction;
