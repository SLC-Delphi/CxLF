PROCEDURE T_FRM_SIM_LOTOFACIL01.PR_Salvar_Historico ();
VAR
     VI_CONTAR: INTEGER;
     VC_QUERY : STRING;
BEGIN
     IF NOT(SELF.IBTransaction1.Active) THEN SELF.IBTransaction1.StartTransaction;
     SELF.IBQ_COMBINACOES.SQL.Clear;
     SELF.IBQ_COMBINACOES.SQL.Add('DELETE FROM HISTORICO1;')  ;
     SELF.IBQ_COMBINACOES.ExecSQL;
     FOR VI_CONTAR := 0 TO MEM_HistoricoFiltro.Lines.Count DO
     BEGIN
          SELF.IBQ_COMBINACOES.SQL.Clear;
          VC_QUERY  := 'INSERT INTO HISTORICO1 (DADOS) VALUES(' +#39+ MEM_HistoricoFiltro.Lines.Strings[VI_CONTAR] + #39 +');';
          SELF.IBQ_COMBINACOES.SQL.Add(VC_QUERY)  ;
          SELF.IBQ_COMBINACOES.ExecSQL;
     END;
     SELF.IBQ_COMBINACOES.SQL.Clear;
     SELF.IBQ_COMBINACOES.SQL.Add('COMMIT;')  ;
     SELF.IBQ_COMBINACOES.ExecSQL;

     SELF.IBQ_COMBINACOES.SQL.Clear;
     SELF.IBQ_COMBINACOES.SQL.Add('DELETE FROM HISTORICO0;')  ;
     SELF.IBQ_COMBINACOES.ExecSQL;
     FOR VI_CONTAR := 0 TO MEM_HistoricoFiltro.Lines.Count DO
     BEGIN
          SELF.IBQ_COMBINACOES.SQL.Clear;
          VC_QUERY  := 'INSERT INTO HISTORICO0 (DADOS) VALUES(' +#39+ MEM_DescFiltro.Lines.Strings[VI_CONTAR] + #39 +');';
          SELF.IBQ_COMBINACOES.SQL.Add(VC_QUERY)  ;
          SELF.IBQ_COMBINACOES.ExecSQL;
     END;
     SELF.IBQ_COMBINACOES.SQL.Clear;
     SELF.IBQ_COMBINACOES.SQL.Add('COMMIT;')  ;
     SELF.IBQ_COMBINACOES.ExecSQL;
END; // PROCEDURE



