     SELF.IBQ_COMBINACOES.ExecSQL;
     LA_LER_GRAVAR_PERCENTUAL_TITULO.Caption := 'GRAVANDO OS DADOS / RESULTADO';
     LA_LER_GRAVAR_PERCENTUAL_TEXTO1.Caption := '.';
     PGBR_LER_GRAVAR_PERCENTUAL.Position     := 0;
     PA_LER_GRAVAR_PERCENTUAL.Left           := 32;
     PA_LER_GRAVAR_PERCENTUAL.Repaint;
     SLEEP (4);
     FOR VI_COMBINACOES1:= 1 TO (STGR_COMBINACOES_15.RowCount-1) DO
     BEGIN
          IF (TRIM(VC_FLAG_MENSAGEM) = 'FLAG16') THEN
               IF (STGR_COMBINACOES_15.Cells[16,VI_COMBINACOES1] = '[F]') THEN
                    CONTINUE;
          VC_DADOS := '';
          IF (STGR_COMBINACOES_15.Cells[40,VI_COMBINACOES1])= '0' THEN
               CONTINUE;
          // --- MONTA OS DADOS A SEREM ARMAZENADOS:
          FOR VI_CONTAR := 1 TO 15 DO
          BEGIN
               VC_DADOS := VC_DADOS + STGR_COMBINACOES_15.Cells[VI_CONTAR,VI_COMBINACOES1] + ' ';
          END;// FOR VI_CONTAR
          VC_QUERY  := 'INSERT INTO A_RESULTADO_15_DADOS (DADOS, NS_A1, NS_A2, NS_A3, NS_B1, NS_B2, NS_B3, NS_C1, NS_C2, NS_D1, NS_D2) VALUES('
                    +#39+ VC_DADOS
                     + #39 +',' +#39+ STGR_COMBINACOES_15.Cells[17,VI_COMBINACOES1]+''
                     + #39 +',' +#39+ STGR_COMBINACOES_15.Cells[18,VI_COMBINACOES1]+''
                     + #39 +',' +#39+ STGR_COMBINACOES_15.Cells[19,VI_COMBINACOES1]+''
                     + #39 +',' +#39+ STGR_COMBINACOES_15.Cells[20,VI_COMBINACOES1]+''
                     + #39 +',' +#39+ STGR_COMBINACOES_15.Cells[21,VI_COMBINACOES1]+''
                     + #39 +',' +#39+ STGR_COMBINACOES_15.Cells[22,VI_COMBINACOES1]+''
                     + #39 +',' +#39+ STGR_COMBINACOES_15.Cells[23,VI_COMBINACOES1]+''
                     + #39 +',' +#39+ STGR_COMBINACOES_15.Cells[24,VI_COMBINACOES1]+''
                     + #39 +',' +#39+ STGR_COMBINACOES_15.Cells[25,VI_COMBINACOES1]+''
                     + #39 +',' +#39+ STGR_COMBINACOES_15.Cells[26,VI_COMBINACOES1]+''
                     + #39 +');';
          SELF.IBQ_COMBINACOES.SQL.Clear;
          SELF.IBQ_COMBINACOES.SQL.Add(VC_QUERY)  ;
          SELF.IBQ_COMBINACOES.ExecSQL;
          SELF.IBQ_COMBINACOES.SQL.Clear;
          SELF.IBQ_COMBINACOES.SQL.Add('COMMIT')  ;
          SELF.IBQ_COMBINACOES.ExecSQL;
               if (VI_COMBINACOES1 MOD 250)=0 then
               BEGIN
                    LA_LER_GRAVAR_PERCENTUAL_TEXTO1.Caption:=  INTtoSTR( TRUNC(VI_COMBINACOES1  / (STGR_COMBINACOES_15.RowCount-1) *100) ) + ' %  ';
                    LA_LER_GRAVAR_PERCENTUAL_TEXTO1.Caption:= LA_LER_GRAVAR_PERCENTUAL_TEXTO1.Caption +' -------  Quantidade:' + INTtoSTR(VI_COMBINACOES1) +' de  '+ INTtoSTR(STGR_COMBINACOES_15.RowCount-1) ;
                    LA_LER_GRAVAR_PERCENTUAL_TEXTO1.Repaint;
                    SLEEP (1);
                    PGBR_LER_GRAVAR_PERCENTUAL.Position := (TRUNC(VI_COMBINACOES1  / (STGR_COMBINACOES_15.RowCount-1) *100) );
                    PGBR_LER_GRAVAR_PERCENTUAL.Repaint;
                    STGR_COMBINACOES_15.REPAINT;
                    SLEEP(1);
               END;
     END;// FOR VI_COMBINACOES1
     LA_LER_GRAVAR_PERCENTUAL_TEXTO1.Caption:=  INTtoSTR( TRUNC(VI_COMBINACOES1  / (STGR_COMBINACOES_15.RowCount-1) *100) ) + ' %  ';
