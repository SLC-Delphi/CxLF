            STGR_SIM_L1_08.Cells [2,VI_LINHAS2+0]:= INTtoSTR(VI_QTD_PARES);

            VI_QTD_PARES   := 0;
            VI_QTD_IMPARES := 0;
            IF ( (STRtoINT(STGR_BASE_L12_SOBRA.Cells[1,0]) /2) = (STRtoINT(STGR_BASE_L12_SOBRA.Cells[1,0]) DIV 2) ) THEN
                VI_QTD_PARES:= VI_QTD_PARES + 1
