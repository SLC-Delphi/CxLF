                    EXIT;
                END ELSE
                IF (VI_CONTAR2 = 5) THEN
                BEGIN
                    FOR VI_CONTAR3 :=1 TO 5 DO
                    BEGIN
                        IF (STRtoINT (STGR_BASE_L3.Cells [VI_CONTAR3,0]) = 0) THEN
                        BEGIN
                            STGR_BASE_L3.Cells [VI_CONTAR3,0] := RIGHTSTR ('0'+INTtoSTR(VI_NUMERO),2);
                            EXIT  ;
                        END ELSE
                        IF (VI_CONTAR3 = 5) THEN
                        BEGIN
                            FOR VI_CONTAR4 :=1 TO 5 DO
                            BEGIN
                                IF (STRtoINT (STGR_BASE_L4.Cells [VI_CONTAR4,0]) = 0) THEN
                                BEGIN
                                    STGR_BASE_L4.Cells [VI_CONTAR4,0] := RIGHTSTR ('0'+INTtoSTR(VI_NUMERO),2);
                                    EXIT ;
                            END ELSE
                                IF (VI_CONTAR4 = 5) THEN
                                BEGIN
                                    FOR VI_CONTAR5 :=1 TO 5 DO
                                    BEGIN
                                        IF (STRtoINT (STGR_BASE_L5.Cells [VI_CONTAR5,0]) = 0) THEN
                                        BEGIN
                                            STGR_BASE_L5.Cells [VI_CONTAR5,0] := RIGHTSTR ('0'+INTtoSTR(VI_NUMERO),2);
                                            EXIT ;
                                        END; // IF VI_CONTAR5
                                    END; // FOR VI_CONTAR5
                                END; // IF VI_CONTAR4
                            END; // FOR VI_CONTAR4
                        END; // IF (VI_CONTAR3
                    END;// FOR VI_CONTAR3
                END; // IF (STRtoINT
            END; // FOR VI_CONTAR 2
        END;// IF (STRtoINT
    END; // VI_CONTAR

END;


procedure T_FRM_SIM_LOTOFACIL01.BBT_SIMULAR_MOD8Click(Sender: TObject);
var
    VI_COMBINACOES1, VI_COMBINACOES2, VI_COMBINACOES3, VI_COMBINACOES4, VI_QTD_CELULAS : SMALLINT;
    VI_ACONTECEU, VI_QTD_PARES, VI_QTD_IMPARES : SMALLINT;
    VI_LINHAS1, VI_LINHAS2 : INTEGER;
begin
    {
