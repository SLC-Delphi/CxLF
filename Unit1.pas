            STGR_SIM_L1_08.Cells [3,VI_LINHAS2+1]:= STGR_BASE_L12_SOBRA.Cells[1,0];
            STGR_SIM_L1_08.Cells [4,VI_LINHAS2+1]:= STGR_BASE_L12_SOBRA.Cells[3,0];
            STGR_SIM_L1_08.Cells [0,VI_LINHAS2+2]:= INTtoSTR(VI_LINHAS2+2);
            STGR_SIM_L1_08.Cells [3,VI_LINHAS2+2]:= STGR_BASE_L12_SOBRA.Cells[2,0];
            STGR_SIM_L1_08.Cells [4,VI_LINHAS2+2]:= STGR_BASE_L12_SOBRA.Cells[3,0];
                // *****************************
                // ** Montando o bloco de  1  **
                // *****************************
            STGR_SIM_L1_01.Cells [0,VI_LINHAS2+0]:= INTtoSTR(VI_LINHAS2);
            STGR_SIM_L1_01.Cells [1,VI_LINHAS2+0]:= STGR_BASE_L12_SOBRA.Cells[3,0];
            STGR_SIM_L1_01.Cells [0,VI_LINHAS2+1]:= INTtoSTR(VI_LINHAS2+1);
            STGR_SIM_L1_01.Cells [1,VI_LINHAS2+1]:= STGR_BASE_L12_SOBRA.Cells[2,0];
            STGR_SIM_L1_01.Cells [0,VI_LINHAS2+2]:= INTtoSTR(VI_LINHAS2+2);
            STGR_SIM_L1_01.Cells [1,VI_LINHAS2+2]:= STGR_BASE_L12_SOBRA.Cells[1,0];
                // -----------------------------------
                // -- Calculando os pares e mpares --
                // -----------------------------------
            VI_QTD_PARES   := 0;
            VI_QTD_IMPARES := 0;
            IF ( (STRtoINT(STGR_BASE_L12_SOBRA.Cells[1,0]) /2) = (STRtoINT(STGR_BASE_L12_SOBRA.Cells[1,0]) DIV 2) ) THEN
                VI_QTD_PARES:= VI_QTD_PARES + 1
            ELSE
                VI_QTD_IMPARES:= VI_QTD_IMPARES + 1;
            IF ( (STRtoINT(STGR_BASE_L12_SOBRA.Cells[2,0]) /2) = (STRtoINT(STGR_BASE_L12_SOBRA.Cells[2,0]) DIV 2) ) THEN
                VI_QTD_PARES:= VI_QTD_PARES + 1
            ELSE
