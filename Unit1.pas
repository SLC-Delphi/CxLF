     FOR VI_COMBINACOES1:= 1 TO (STGR_COMBINACOES_15.RowCount-1) DO
     BEGIN
          IF (VC_FLAG_MENSAGEM = 'FLAG16') THEN
               IF (trim(STGR_COMBINACOES_15.Cells[16,VI_COMBINACOES1]) = '[F]') THEN
                    CONTINUE;
          VC_DADOS := '';
          // --- MONTA OS DADOS A SEREM ARMAZENADOS:
