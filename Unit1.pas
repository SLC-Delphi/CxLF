    PGBR_FIL08_GERARCOMB.Position := 0;
    PGBR_FIL08_REMREPET.Position := 0;
    PGBR_FIL08_FILTRARCOMB.Position := 0;
    STGR_SIM_12_SOMATORIA.Cells[0,0] := '0';
    STGR_SIM_12_SOMATORIA.Cells[0,1] := '0';
    STGR_SIM_08_SOMATORIA.Cells[0,0] := '0';
    STGR_SIM_08_SOMATORIA.Cells[0,1] := '0';

    VI_LINHAS1:= 1;
    VI_LINHAS2:= 1;
    STGR_SIM_L1_12.Cells [0,0]:= 'NR';
    STGR_SIM_L1_12.Cells [1,0]:= 'IMP';
    STGR_SIM_L1_12.Cells [2,0]:= 'PAR';
    STGR_SIM_L2_12.Cells [0,0]:= 'NR';
    STGR_SIM_L2_12.Cells [1,0]:= 'IMP';
    STGR_SIM_L2_12.Cells [2,0]:= 'PAR';
    STGR_SIM_L3_12.Cells [0,0]:= 'NR';
    STGR_SIM_L3_12.Cells [1,0]:= 'IMP';
    STGR_SIM_L3_12.Cells [2,0]:= 'PAR';
    STGR_SIM_L4_12.Cells [0,0]:= 'NR';
    STGR_SIM_L4_12.Cells [1,0]:= 'IMP';
    STGR_SIM_L4_12.Cells [2,0]:= 'PAR';
    STGR_SIM_L5_12.Cells [0,0]:= 'NR';
    STGR_SIM_L5_12.Cells [1,0]:= 'IMP';
    STGR_SIM_L5_12.Cells [2,0]:= 'PAR';

    STGR_SIM_L1_08.Cells [0,0]:= 'NR';
    STGR_SIM_L1_08.Cells [1,0]:= 'IMP';
    STGR_SIM_L1_08.Cells [2,0]:= 'PAR';
    STGR_SIM_L2_08.Cells [0,0]:= 'NR';
    STGR_SIM_L2_08.Cells [1,0]:= 'IMP';
    STGR_SIM_L2_08.Cells [2,0]:= 'PAR';
    STGR_SIM_L3_08.Cells [0,0]:= 'NR';
    STGR_SIM_L3_08.Cells [1,0]:= 'IMP';
    STGR_SIM_L3_08.Cells [2,0]:= 'PAR';
    STGR_SIM_L4_08.Cells [0,0]:= 'NR';
    STGR_SIM_L4_08.Cells [1,0]:= 'IMP';
    STGR_SIM_L4_08.Cells [2,0]:= 'PAR';
    STGR_SIM_L5_08.Cells [0,0]:= 'NR';
    STGR_SIM_L5_08.Cells [1,0]:= 'IMP';
    STGR_SIM_L5_08.Cells [2,0]:= 'PAR';
    STGR_SIM_12_SOMATORIA.Cells [1,0]:= 'IMP';
    STGR_SIM_12_SOMATORIA.Cells [2,0]:= 'PAR';
    STGR_SIM_12_SOMATORIA.Cells [3,0]:= 'TOTAL';
    STGR_SIM_08_SOMATORIA.Cells [1,0]:= 'IMP';
    STGR_SIM_08_SOMATORIA.Cells [2,0]:= 'PAR';
    STGR_SIM_08_SOMATORIA.Cells [3,0]:= 'TOTAL';
    // =====================================================================
    // ||||=============================================================||||
    // |||| 	Anlises Combinatrias e suas sobras - Linhas mpares   ||||
    // ||||=============================================================||||
    // =====================================================================
    FOR VI_COMBINACOES1:= 1 TO 5 DO
    BEGIN
        FOR VI_COMBINACOES2:= VI_COMBINACOES1+1 TO 5 DO
        BEGIN
            // ***************************************
            // ** LINHA 1 - Montando o bloco de 12  **
            // ***************************************
            STGR_SIM_L1_12.Cells [0,VI_LINHAS2+0]:= INTtoSTR(VI_LINHAS1)+'/'+INTtoSTR(VI_LINHAS2+0);
            STGR_SIM_L1_12.Cells [3,VI_LINHAS2+0]:= STGR_BASE_L1.Cells[VI_COMBINACOES1,0];
            STGR_SIM_L1_12.Cells [4,VI_LINHAS2+0]:= STGR_BASE_L1.Cells[VI_COMBINACOES2,0];
            STGR_SIM_L1_12.Cells [0,VI_LINHAS2+1]:= INTtoSTR(VI_LINHAS1)+'/'+INTtoSTR(VI_LINHAS2+1);
            STGR_SIM_L1_12.Cells [3,VI_LINHAS2+1]:= STGR_BASE_L1.Cells[VI_COMBINACOES1,0];
            STGR_SIM_L1_12.Cells [4,VI_LINHAS2+1]:= STGR_BASE_L1.Cells[VI_COMBINACOES2,0];
            STGR_SIM_L1_12.Cells [0,VI_LINHAS2+2]:= INTtoSTR(VI_LINHAS1)+'/'+INTtoSTR(VI_LINHAS2+2);
            STGR_SIM_L1_12.Cells [3,VI_LINHAS2+2]:= STGR_BASE_L1.Cells[VI_COMBINACOES1,0];
            STGR_SIM_L1_12.Cells [4,VI_LINHAS2+2]:= STGR_BASE_L1.Cells[VI_COMBINACOES2,0];
                // -----------------------------------
                // -- Calculando os pares e mpares --
                // -----------------------------------
            VI_QTD_PARES   := 0;
            VI_QTD_IMPARES := 0;
            IF ( (STRtoINT(STGR_BASE_L1.Cells[VI_COMBINACOES1,0]) /2) = (STRtoINT(STGR_BASE_L1.Cells[VI_COMBINACOES1,0]) DIV 2) ) THEN
                VI_QTD_PARES:= VI_QTD_PARES + 1
            ELSE
                VI_QTD_IMPARES:= VI_QTD_IMPARES + 1;

            IF ( (STRtoINT(STGR_BASE_L1.Cells[VI_COMBINACOES2,0]) /2) = (STRtoINT(STGR_BASE_L1.Cells[VI_COMBINACOES2,0]) DIV 2) ) THEN
                VI_QTD_PARES:= VI_QTD_PARES + 1
            ELSE
                VI_QTD_IMPARES:= VI_QTD_IMPARES + 1;

            STGR_SIM_L1_12.Cells [1,VI_LINHAS2+0]:= INTtoSTR(VI_QTD_IMPARES);
            STGR_SIM_L1_12.Cells [1,VI_LINHAS2+1]:= INTtoSTR(VI_QTD_IMPARES);
            STGR_SIM_L1_12.Cells [1,VI_LINHAS2+2]:= INTtoSTR(VI_QTD_IMPARES);
            STGR_SIM_L1_12.Cells [2,VI_LINHAS2+0]:= INTtoSTR(VI_QTD_PARES);
            STGR_SIM_L1_12.Cells [2,VI_LINHAS2+1]:= INTtoSTR(VI_QTD_PARES);
            STGR_SIM_L1_12.Cells [2,VI_LINHAS2+2]:= INTtoSTR(VI_QTD_PARES);
                // -----------------------
                // -- Pegando as sobras --
                // -----------------------
            VI_QTD_CELULAS:= 1;
            FOR VI_COMBINACOES3:= 1 TO 5 DO
            BEGIN
                IF (STGR_BASE_L1.Cells[VI_COMBINACOES3,0] <> STGR_SIM_L1_12.Cells [3,VI_LINHAS2]) AND
                    (STGR_BASE_L1.Cells[VI_COMBINACOES3,0] <> STGR_SIM_L1_12.Cells [4,VI_LINHAS2])
                THEN
                BEGIN
                    STGR_BASE_L12_SOBRA.Cells [VI_QTD_CELULAS,0]:= STGR_BASE_L1.Cells[VI_COMBINACOES3,0];
                     VI_QTD_CELULAS:= VI_QTD_CELULAS + 1;
                END;
            END;
                // *****************************
                // ** Montando o bloco de  8  **
                // *****************************
