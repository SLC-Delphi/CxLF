          PROCEDURE T_FRM_SIM_LOTOFACIL01.PR_100REPET_EXCLUIDOS (VC_TIPO: STRING);
          VAR
               VI_COMBINACOES1, VI_COMBINACOES2, VI_CONTAR, VI_QTDFILTRAR, VI_FINAL : integer;
          BEGIN
               //----------------------------------------------------
               // Procura repetidos e assinala-os como F na coluna 35
               //----------------------------------------------------
               PGBR_EXCLUIDOS_ETAPAS.Position := 0;
               STGR_COMBINACOES_15.Cells[40,0]:= '1';
               VI_FINAL :=  (STGR_COMBINACOES_15.RowCount-1);
               IF (VC_TIPO = '1000') THEN
                    VI_QTDFILTRAR := 1000;
               IF (VC_TIPO = '2000') THEN
                    VI_QTDFILTRAR := 2000;
               FOR VI_COMBINACOES1:= 1 TO (STGR_COMBINACOES_15.RowCount-1) DO
               BEGIN
                         IF (STGR_COMBINACOES_15.Cells[40,VI_COMBINACOES1] = '0') THEN
                              CONTINUE;
                         STGR_COMBINACOES_15.Cells[40,VI_COMBINACOES1] := '1';              // CARREGAR COMO 1
                         IF (STGR_COMBINACOES_15.Cells[41,VI_COMBINACOES1] = 'V') THEN       // CARREGAR COMO VERDADEIRA
                         BEGIN
                              IF (VC_TIPO='') THEN
                              BEGIN
                                   VI_FINAL :=  (STGR_COMBINACOES_15.RowCount-1);
                              END ELSE
                              BEGIN
                                   VI_FINAL := VI_COMBINACOES1+ VI_QTDFILTRAR;
                                   IF (VI_FINAL > STGR_COMBINACOES_15.RowCount-1) THEN VI_FINAL :=  (STGR_COMBINACOES_15.RowCount-1);
                              END; //IF (
                              FOR VI_COMBINACOES2:= VI_COMBINACOES1+1 TO VI_FINAL DO
                              BEGIN
                                    IF  (STGR_COMBINACOES_15.Cells[1,VI_COMBINACOES1] = STGR_COMBINACOES_15.Cells[1,VI_COMBINACOES2]) AND
                                            (STGR_COMBINACOES_15.Cells[2,VI_COMBINACOES1] = STGR_COMBINACOES_15.Cells[2,VI_COMBINACOES2])
                                        THEN
                                    BEGIN
                                        IF  (STGR_COMBINACOES_15.Cells[3,VI_COMBINACOES1] = STGR_COMBINACOES_15.Cells[3,VI_COMBINACOES2]) AND
                                            (STGR_COMBINACOES_15.Cells[4,VI_COMBINACOES1] = STGR_COMBINACOES_15.Cells[4,VI_COMBINACOES2]) AND
                                            (STGR_COMBINACOES_15.Cells[5,VI_COMBINACOES1] = STGR_COMBINACOES_15.Cells[5,VI_COMBINACOES2]) AND
                                            (STGR_COMBINACOES_15.Cells[6,VI_COMBINACOES1] = STGR_COMBINACOES_15.Cells[6,VI_COMBINACOES2]) AND
                                            (STGR_COMBINACOES_15.Cells[7,VI_COMBINACOES1] = STGR_COMBINACOES_15.Cells[7,VI_COMBINACOES2]) AND
                                            (STGR_COMBINACOES_15.Cells[8,VI_COMBINACOES1] = STGR_COMBINACOES_15.Cells[8,VI_COMBINACOES2]) AND
                                            (STGR_COMBINACOES_15.Cells[9,VI_COMBINACOES1] = STGR_COMBINACOES_15.Cells[9,VI_COMBINACOES2]) AND
                                            (STGR_COMBINACOES_15.Cells[10,VI_COMBINACOES1] = STGR_COMBINACOES_15.Cells[10,VI_COMBINACOES2]) AND
                                            (STGR_COMBINACOES_15.Cells[11,VI_COMBINACOES1] = STGR_COMBINACOES_15.Cells[11,VI_COMBINACOES2]) AND
                                            (STGR_COMBINACOES_15.Cells[12,VI_COMBINACOES1] = STGR_COMBINACOES_15.Cells[12,VI_COMBINACOES2]) AND
                                            (STGR_COMBINACOES_15.Cells[13,VI_COMBINACOES1] = STGR_COMBINACOES_15.Cells[13,VI_COMBINACOES2]) AND
                                            (STGR_COMBINACOES_15.Cells[14,VI_COMBINACOES1] = STGR_COMBINACOES_15.Cells[14,VI_COMBINACOES2]) AND
                                            (STGR_COMBINACOES_15.Cells[15,VI_COMBINACOES1] = STGR_COMBINACOES_15.Cells[15,VI_COMBINACOES2])
                                            THEN
                                        BEGIN
                                            STGR_COMBINACOES_15.Cells[40,VI_COMBINACOES2] := '0'
                                        END; // if (STGR_GRUPO_D_TODOS3.Cells[1,VI_CONTAR]
                                    END; // if (STGR_GRUPO_D_TODOS3.Cells[1,VI_CONTAR]
                              END; // FOR VI_COMBINACOES2
                         END; // IF
                    if (VI_COMBINACOES1/100)=(VI_COMBINACOES1 DIV 100) then
                    BEGIN
                         PGBR_EXCLUIDOS_ETAPAS.Position := (TRUNC(VI_COMBINACOES1  / (STGR_COMBINACOES_15.RowCount-1) *100) );
                         PGBR_EXCLUIDOS_ETAPAS.Repaint;
                         SLEEP(2);
                    END;
                    if (VI_COMBINACOES1/1000)=(VI_COMBINACOES1 DIV 1000) then
                    BEGIN
                         Self.WindowState := wsMinimized;
                         Self.WindowState :=  wsNormal;
                         SELF.Repaint;
                    END; // IF (VI_COMBINACOES1/1000
               END; // FOR VI_COMBINACOES1
               VI_CONTAR := 0;
               FOR VI_COMBINACOES1:= 0 TO (STGR_COMBINACOES_15.RowCount-1) DO
               BEGIN
                    IF (STGR_COMBINACOES_15.Cells[40,VI_COMBINACOES1] = '1') AND (STGR_COMBINACOES_15.Cells[41,VI_COMBINACOES1] = 'V') THEN VI_CONTAR:=VI_CONTAR +1;
                    //IF  (STGR_COMBINACOES_15.Cells [36,VI_COMBINACOES1] = 'V') THEN VI_CONTAR:=VI_CONTAR +1;
               END;
               PGBR_EXCLUIDOS_ETAPAS.Position := 100;
               STGR_COMBINACOES_15.ColCount := 41;
               SELF.Repaint;
               LA_QTD_RESULTADO.Caption := INTtoSTR(STGR_COMBINACOES_15.RowCount)+'    ';
               LA_QTD_RESULTADO.Repaint;
               LA_QTD_RESULTADO.Caption := LA_QTD_RESULTADO.Caption +' '+ 'TOTAL COMPATVEIS: '+INTtoSTR(VI_CONTAR)+'  ';
               STGR_COMBINACOES_15.Repaint;
          end; // PR_100REPET_EXCLUIDOS





